{
  "name": "@applet/icons",
  "version": "0.0.1",
  "sideEffects": false,
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "es",
    "inline-svg",
    "inline-namespaced-svg"
  ],
  "scripts": {
    "g": "npm run generate",
    "generate": "cross-env NODE_ENV=production gulp --require ts-node/register/transpile-only",
    "type-check": "tsc --noEmit",
    "build:es": "tsc --module esnext --outDir es",
    "build:lib": "tsc --module commonjs --outDir lib",
    "build": "cross-env NODE_ENV=production && npm run build:es && npm run build:lib",
    "test": "cross-env NODE_ENV=test && npm run test:unit",
    "test:unit": "jest --passWithNoTests",
    "prepublishOnly": "npm run test && npm run g && npm run build && npm run type-check"
  },
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/preset-env": "^7.10.0",
    "@babel/preset-typescript": "^7.9.0",
    "@rgrove/parse-xml": "^2.0.4",
    "@types/gulp": "^4.0.6",
    "@types/gulp-concat": "^0.0.32",
    "@types/gulp-rename": "^0.0.33",
    "@types/jest": "27.0.1",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.template": "^4.4.6",
    "@types/lodash.upperfirst": "^4.3.6",
    "@types/node": "16.7.13",
    "@types/ramda": "^0.27.6",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@types/rgrove__parse-xml": "^1.1.0",
    "@types/svgo": "^1.3.3",
    "@types/through2": "^2.0.36",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "babel-jest": "27.4.2",
    "cross-env": "^7.0.3",
    "del": "^5.1.0",
    "eslint": "8.3.0",
    "globby": "^11.0.0",
    "gulp": "^4.0.2",
    "gulp-clone": "^2.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-header": "^2.0.9",
    "gulp-rename": "^2.0.0",
    "jest": "27.4.3",
    "lodash.camelcase": "^4.3.0",
    "lodash.template": "^4.5.0",
    "lodash.upperfirst": "^4.3.1",
    "ramda": "^0.27.0",
