{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "python代码执行",
    "description": "",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "minLength": 1,
            "maxLength": 255
        },
        "title": {
            "type": "string"
        },
        "operator": {
            "type": "string",
            "pattern": "^@.*",
            "maxLength": 255
        },
        "parameters": {
            "type": "object",
            "properties": {
                "mode": {
                    "type": "string",
                    "enum": [
                        "sync",
                        "async"
                    ]
                },
                "input_params": {
                    "type": "array",
                    "items": {
                        "properties": {
                            "type": {
                                "type": "string"
                            },
                            "key": {
                                "type": "string"
                            },
                            "value": {
                                "type": "string"
                            }
                        }
                    }
                },
                "output_params": {
                    "type": "array",
                    "items": {
                        "properties": {
                            "type": {
                                "type": "string"
                            },
                            "key": {
                                "type": "string"
                            }
                        }
                    }
                },
                "code": {
                    "type": "string"
                }
            },
            "required": [
                "input_params",
                "output_params",
                "code"
            ],
            "additionalProperties": false
        },
        "settings":{
            "$ref": "../common.json#/definitions/Settings"
        }
    },
    "required": [
        "id",
        "operator",
        "parameters"
    ],
    "additionalProperties": false
}