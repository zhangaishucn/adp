{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "更新自定义节点",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "maxLength": 256
        },
        "description": {
            "type": "string",
            "maxLength": 1024
        },
        "accessors": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "type": {
                        "type": "string",
                        "enum": [
                            "user",
                            "department",
                            "contactor",
                            "group"
                        ]
                    },
                    "id": {
                        "type": "string"
                    }
                },
                "required": [
                    "id",
                    "type"
                ]
            },
            "uniqueItems": true
        },
        "status": {
            "type": "integer",
            "enum": [
                0,
                1
            ]
        }
    },
    "additionalProperties": false
}