{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "授权",
    "description": "post /oauth2/auth",
    "type": "object",
    "properties": {
        "redirect_uri": {
            "description": "授权成功后返回的页面地址",
            "type": "string",
            "pattern": "(https?|ftp|file)://[-A-Za-z0-9+&@#/%?=~_|!:,.;]+[-A-Za-z0-9+&@#/%=~_|]"
        }
    },
    "required": ["redirect_uri"],
    "additionalProperties": false
}