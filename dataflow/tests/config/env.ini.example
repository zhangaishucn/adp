[server]
# 服务器地址（根据实际环境修改）
host = 192.168.232.11
port = 443
# 数据库端口
db_port = 3330
# 数据库用户名
db_user = anyshare
# 数据库密码（请替换为实际密码）
db_pwd = YOUR_DATABASE_PASSWORD_HERE

[requests]
# 请求协议
protocol = https

[hydra]
# Hydra 服务名称
svc_name = hydra-admin
# Hydra 服务端口
svc_port = 4445

[admin]
# 管理员账户密码（用于测试自动化）
admin_password = YOUR_ADMIN_PASSWORD_HERE
