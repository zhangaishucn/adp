{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Operator_Register_Success_Response",
    "type": "array",
    "items": {
        "anyOf": [
            {
                "type": "integer",
                "const": 200,
                "description": "The response status code."
            },
            {
                "type": "array",
                "properties": {
                    "status": {
                        "type": "string",
                        "enum": ["success", "failed"]
                    },
                    "operator_id": {
                        "type": "string"
                    },
                    "version": {
                        "type": "string"
                    },
                    "error": {
                        "type": "object",
                        "properties": {
                            "code": {
                                "type": "string"
                            },
                            "description": {
                                "type": "string"
                            },
                            "detail": {
                                "type": "object"
                            },
                            "solution": {
                                "type": "string"
                            },
                            "link": {
                                "type": "string"
                            }
                        }
                    }
                },
                "required": ["status"]
            }
        ]
    }
}