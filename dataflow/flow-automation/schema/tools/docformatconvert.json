{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Doc format convert",
    "description": "",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "minLength": 1,
            "maxLength": 255
        },
        "title": {
            "type": "string"
        },
        "operator": {
            "type": "string",
            "pattern": "^@.*",
            "maxLength": 255
        },
        "parameters": {
            "type": "object",
            "properties": {
                "docid": {
                    "type": "string"
                },
                "version": {
                    "type": "string"
                }
            },
            "required": [
                "docid"
            ]
        },
        "settings":{
            "$ref": "../common.json#/definitions/Settings"
        }
    },
    "required": [
        "id",
        "operator",
        "parameters"
    ],
    "additionalProperties": false
}