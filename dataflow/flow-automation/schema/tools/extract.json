{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "自定义模型提取",
    "description": "",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "minLength": 1,
            "maxLength": 255
        },
        "title": {
            "type": "string"
        },
        "operator": {
            "type": "string",
            "pattern": "^@.*",
            "maxLength": 255
        },
        "parameters": {
            "type": "object",
            "properties": {
                "modelid":{
                    "type": "string",
                    "pattern": "^\\d+$"
                },
                "content": {
                    "type": "string"
                },
                "type": {
                    "type": "integer",
                    "enum": [1,2]
                }
            },
            "required": [
                "modelid",
                "content",
                "type"
            ],
            "additionalProperties": false
        },
        "settings":{
            "$ref": "../common.json#/definitions/Settings"
        }
    },
    "required": [
        "id",
        "operator",
        "parameters"
    ],
    "additionalProperties": false
}