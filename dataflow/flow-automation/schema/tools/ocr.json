        },
        "parameters": {
            "type": "object",
            "properties": {
                "docid": {
                    "type": "string"
                }
            },
            "required": [
                "docid"
            ],
            "additionalProperties": false
        }
    },
    "required": [
        "id",
        "operator",
        "parameters"
    ],
    "additionalProperties": false
}