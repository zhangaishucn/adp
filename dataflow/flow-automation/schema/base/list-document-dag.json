{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "列举任务",
    "description": "get /api/automation/v1/dags",
    "type": "object",
    "properties": {
        "page": {
            "description": "页码，默认值为0",
            "oneOf": [
                {
                    "type": "integer",
                    "minimum": 0
                },
                {
                    "type": "null"
                }
            ]
        },
        "limit": {
            "description": "每页显示条目数，默认值为20，有效值在1-100之间",
            "oneOf": [
                {
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 100
                },
                {
                    "const": -1
                },
                {
                    "type": "null"
                }
            ]
        },
        "keyword": {
            "description": "关键字，用于搜索筛选任务名称",
            "oneOf": [
                {
                    "type": "string"
                },
                {
                    "type": "null"
                }
            ]
        },
        "sortby": {
            "description": "排序字段，默认为update_at,仅支持name、created_at和updated_at",
            "enum": [
                null,
                "name",
                "created_at",
                "updated_at"
            ]
        },
        "order": {
            "description": "排序规则。默认为desc，仅支持desc和asc",
            "enum": [
                null,
                "asc",
                "desc"
            ]
        },
        "docid": {
            "description": "触发文件ID",
            "type": "string",
            "pattern": "^gns://([A-Z0-9]{32}/?)+$"
        }
    },
    "additionalProperties": false
}