{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "列举任务",
    "description": "get /api/automation/v1/tags/rule",
    "type": "object",
    "properties": {
        "page": {
            "description": "页码，默认值为0",
            "oneOf": [
                {
                    "type": "integer",
                    "minimum": 0
                },
                {
                    "type": "null"
                }
            ]
        },
        "limit": {
            "description": "每页显示条目数，默认值为20，有效值在1-100之间",
            "oneOf": [
                {
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 100
                },
                {
                    "const": -1
                },
                {
                    "type": "null"
                }
            ]
        },
        "status": {
            "description": "规则状态，0代表未启用，1代表已启用",
            "oneOf": [
                {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 1
                },
                {
                    "const": -1
                },
                {
                    "type": "null"
                }
            ]
        }
    },
    "additionalProperties": false
}