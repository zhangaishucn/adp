{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "自定义模型提取参数校验",
    "description": "post /api/automation/v1/uie/infer",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "pattern": "^\\d+$"
        },
        "docid": {
            "type": "string",
            "pattern": "^gns://([A-Z0-9]{32}/?)+$"
        }
    },
    "required": [
        "id",
        "docid"
    ],
    "additionalProperties": false
}