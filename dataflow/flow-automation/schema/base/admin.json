{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "添加管理员",
    "description": "post /api/automation/v1/admin",
    "type": "object",
    "properties": {
        "users": {
            "description": "用户列表",
            "type": "array",
            "items": {
                "type":"object",
                "properties": {
                    "user_id": {
                        "type": "string",
                        "pattern": "^[a-z0-9]{8}-[a-z0-9]{4}-[a-z0-9]{4}-[a-z0-9]{4}-[a-z0-9]{12}$"
                    },
                    "user_name": {
                        "type": "string",
                        "pattern": "^[^/:\\*?\"<>\\|]+$",
                        "maxLength": 128
                    }
                },
                "required": ["user_id", "user_name"],
                "additionalProperties": false
            },
            "uniqueItems": true,
            "minItems": 1
        }
    },
    "required": [
        "users"
    ],
    "additionalProperties": false
}